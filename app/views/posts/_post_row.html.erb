<tr>
    <td>
        <%= link_to post.title, post %>
    </td>
    <td>
        <%= post.body %>
    </td>
    <td>
        <%= post.created_at %>
    </td>
    <td>
        <%= link_to "Edit", edit_post_path(post), class: "button", data: { turbo_frame: "edit_post_#{post.id}"} %>
        <%= button_to "Destroy", post, method: :delete, data: {turbo_confirm: "You sure?"} %>
    </td>
</tr>